function filetag = GenFiletag(roi, arena)
    filetag = [];
    for mRoi = 1 : length(roi)
        if mRoi == 1
            filetag = ['.', char(roi{1})];
        end
        if mRoi > 1
            filetag = [filetag, '.', char(roi{mRoi})];
        end
    end
    for lArena = 1 : length(arena)
        filetag = [filetag, '.', char(arena{lArena})];
    end
end